{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "functions": {
    "api/index.py": {
      "includeFiles": [
        "capstone_hierarchical_rag/**/*.py",
        "capstone_hierarchical_rag/meeting_dates_minmax.txt",
        "templates/**",
        "static/**"
      ]
    }
  }
} 